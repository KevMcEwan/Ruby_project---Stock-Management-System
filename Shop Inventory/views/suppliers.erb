<br>


<form method="get" action="/suppliers/add_new_supplier">
  <button>Add New Supplier</button>
</form>

<br>

<table cellpadding=10px>
  <tr>
    <th>Supplier Name</th>
    <th>Supplies Type</th>
    <th>Contact Name</th>
    <th>Email</th>
    <th>Phone</th>
  </tr>
  <% @suppliers.each do |supplier| %>
    <tr>
      <td><a href="suppliers/<%=supplier.id%>"  "><%= supplier.supplier_name %></a></td>
      <td><%= supplier.supplies_type %></td>
      <td><%= supplier.contact_name %></td>
      <td><%= supplier.email %></td>
      <td><%= supplier.phone %></td>
    </tr>
  <% end %>
</table>
